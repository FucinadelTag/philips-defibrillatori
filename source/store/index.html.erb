<p data-fc-id="minicart">
    <a href="https://defibrillatori-online.foxycart.com/cart?cart=view">
        <span data-fc-id="minicart-quantity">0</span>
        <span data-fc-id="minicart-singular"> item </span>
        <span data-fc-id="minicart-plural"> items </span>
        in cart. Total cost: $
        <span data-fc-id="minicart-order-total">0</span>
    </a>
</p>

<div class="row">
  <div class="large-12 columns">

    <ul class="small-block-grid-1">
        <% find_resource_by_path('^store/consumabili/*', 'price').each do |prodotto| %>
        <li class="panel">
            <div class="row">
                <div class="large-12 columns">
                    <h3 class="subheader"><%= prodotto.data.title %></h3>
                </div>
            </div>
            <div class="row">
                <div class="large-4 columns">
                    <img src="<%= prodotto.data.immagine %>" class="padding" alt="<%= prodotto.data.title %>" />
                </div>
                <div class="large-8 columns">
                    <div class="row">
                        <div class="large-6 columns">
                            <span class="prezzo" ><%= number_to_currency(prodotto.data.price, :unit => "&euro;", :separator => ",", :delimiter => ".") %></span>
                                <br />
                            <span class="less_important" >Codice Prodotto: <%= prodotto.data.codice %></span>
                        </div>

                        <div class="large-6 columns">

                    <!-- form example -->
                        <form action="https://defibrillatori-online.foxycart.com/cart" method="post" accept-charset="utf-8">
                        <input type="hidden" name="name" value="<%= prodotto.data.title %>" />
                        <input type="hidden" name="price" value="<%= prodotto.data.price %>" />
                        <input type="hidden" name="code" value="<%= prodotto.data.codice %>" />
                        <input type="hidden" name="image" value="<%= prodotto.data.immagine %>" />
                        <!-- <input type="hidden" name="category" value="<%= prodotto.data.categoria.downcase %>" /> -->
                        <input type="submit" value="Acquista Subito" class="button round" />
                        </form>

                        </div>
                    </div>

                    <hr />
                     <div class="row">
                        <div class="large-12 columns">
                            <%= prodotto.data.description %>
                        </div>
                    </div>
                </div>
            </div>


        </li>

        <% end %>
    </ul>
  </div>
</div>
